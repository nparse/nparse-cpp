S1 := ^\d+ % ,*0 { res: $s + $1 + $2 } ^$;
S2 := ^\d+ % ,*0 { res: $i + $i1 + $i2 } ^$;
S3 := ^\d+ % ,*0 { res: "<$><$1><$2>" } ^$;

S4 := ^\w+ $S4a { res: "${}-${1}-${3}" } ^$;
S4a := ^\w+ ^\w+;

S5 := "^\w+"* ^\d+ { res: $($i) } ^$;

S6 := ^\w+ + { res: "$0-$1-$2-$3-$4" } ^$;

W := ^\w+;
S7a := $W+ { res: "$0-$1-$2-$3-$4-$5-$6-$7-$8" } ^$;
S7b := <W>+ { res: "$0-$1-$2-$3-$4-$5-$6-$7-$8" } ^$;
